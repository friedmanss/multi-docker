{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [{
      "name": "client",
      "image": "friedmanss/multi-client",
      "hostname": "client",
      "essential": false
    },
    {
      "name": "server",
      "image": "friedmanss/multi-server",
      "hostname": "api",
      "essential": false
    }, {
      "name": "worker",
      "image": "friedmanss/multi-worker",
      "hostname": "worker",
      "essential": false
    }, {
      "name": "nginx",
      "image": "friedmanss/multi-nginx",
      "essential": true,
      "portMappings": [{
        "hostport": 80,
        "containerPort": 80
      }],
      "links": [
        "client", "server"
      ]
    }
  ]
}